{
   "background": {
      "scripts": ["background.js" ]
   },
   "content_scripts":[
      {
         "matches": ["file:///*"],
         "js": [ "jquery.js", "script.js"],
         "run_at": "document_end"
      },
      {
         "matches": ["<all_urls>"],
         "js": [ "jquery.js", "blob.js"],
         "css": ["blob.css"],
         "run_at": "document_end"
      }
   ],
   "web_accessible_resources": [
      "GothamRoundedLight.otf"
   ],
   "manifest_version": 2,
   "name": "Startpage",
   "options_page": "settings.htm",
   "permissions": [ "tabs", "storage", "file://*/*"],
   "version": "1"
}
