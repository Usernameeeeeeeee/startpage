{
   "background": {
      "scripts": ["background.js" ]
   },
   "content_scripts":[
      {
         "matches": ["file:///*"],
         "js": [ "jquery.js", "script.js"],
         "run_at": "document_end"
      }
   ],
   "manifest_version": 2,
   "name": "Startpage",
   "options_page": "settings.htm",
   "permissions": [ "tabs", "storage" ],
   "version": "1"
}
